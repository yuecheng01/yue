<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">


        <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
                <!-- Customize your theme here. -->
                <item name="colorPrimary">@color/colorAccentblue</item>
                <item name="android:textColorPrimary">@color/black</item>
                <item name="colorPrimaryDark">@color/colorAccentblue</item>
                <item name="colorAccent">@color/colorAccentblue</item>
                <item name="android:textColorHint">@color/white</item>
                <item name="android:colorControlNormal" tools:targetApi="lollipop">@color/white</item>
                <item name="android:windowTranslucentStatus">true</item>
                <item name="android:textColorSecondary">@color/white</item>
                <!--自定义颜色-->
                <item name="dialogTextColor">@color/colorAccentblue</item>

        </style>
        <style name="AppTheme2" parent="Theme.AppCompat.Light.NoActionBar">
                <!-- Customize your theme here. -->
                <item name="colorPrimary">@color/colorAccentred</item>
                <item name="android:textColorPrimary">@color/black</item>
                <item name="colorPrimaryDark">@color/colorAccentred</item>
                <item name="colorAccent">@color/colorAccentred</item>
                <item name="android:textColorHint">@color/white</item>
                <item name="android:colorControlNormal" tools:targetApi="lollipop">@color/white</item>
                <item name="android:colorControlActivated" tools:targetApi="lollipop">@color/white</item>
                <item name="android:colorControlHighlight" tools:targetApi="lollipop">@color/white</item>
                <item name="android:windowTranslucentStatus" tools:targetApi="kitkat">true</item>
                <item name="android:textColorSecondary">@color/white</item>
                <!--自定义颜色-->
                <item name="dialogTextColor">@color/colorAccentred</item>
        </style>
</resources>